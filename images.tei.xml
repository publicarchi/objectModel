         <listObject type="albumPlates">
            <!-- type albumPlates ou Items ? ou alors est-ce qu'on type directement l'objet ? -->
            <head>Planches du recueil</head>
            <!-- première liste d'objets correspond aux planches du recueil -->

            <!-- exemple d'encodage des planches se référant à l’hôtel Thélusson (ex. INHA - Fol Res 169) -->
            <object xml:id="FolRes169-pl050">
               <!-- https://sharedocs.huma-num.fr/wl/?id=uv0tvIxjNQ7DboQQAIPLY6KffsKefQW2&path=IMG_1630.jpg -->
               <objectIdentifier>
                  <!-- Lorsque la gravure n'a pas de titre, préciser qu'il est forgé avec @type="forged" -->
                  <objectName type="forged">Vue perspective de l’hôtel Thélusson depuis la grotte</objectName>
                  <!-- quand la planche a une cote, la préciser dans idno -->
                  <idno>FolRes169-pl050</idno>
               </objectIdentifier>
               <objectClassification>
                  <!-- informations relatives à la catégorisation et à la classification de l'objet -->
                  <!-- attributs du système att.canonical : @key pour préciser le nom de l'entité ou du vocabulaire
                                                            @ref pour indiquer son uri -->
                  <!-- category correspond soit à l'entité E22 Human-Made Object soit à E20 Biological Object -->
                  <category key="E22-human-made-object" ref="http://www.cidoc-crm.org/Entity/e22-man-made-object/version-6.2.2"><desc>Human-Made Object</desc></category>
                  <!-- classification est répétable -->
                  <!-- définir le vocabulaire à utiliser -->
                  <classification key="nomVocabulaire" ref="lienVocabulaire">Gravure</classification>
               </objectClassification>
               <physDesc>
                  <objectDesc>
                     <!-- description matérielle -->
                     <mediumDesc>
                        <!-- description de la technique -->
                        <!-- attributs : @type pour préciser "main" ou "sub" quand il y a plusieurs techniques 
                                         @key pour le nom du vocabulaire
                                         @ref pour indiquer l'uri du vocabulaire -->
                        <medium key="nomVocabulaire" ref="lienVocabulaire">gravure ...</medium>
                     </mediumDesc>
                     <supportDesc>
                        <support>
                           <!-- description du support sur lequel l'estampe a été imprimée -->
                           <material>papier...</material>
                           <stamp>estampille des collections Jacques Doucet</stamp>
                           <watermark/>
                        </support>
                        <extent>
                           <!-- dimensions de la feuille imprimée -->
                           <dimensions type="sheet" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <!-- dimensions de la cuvette/plaque -->
                           <dimensions type="plate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                        </extent>
                     </supportDesc>
                     <layoutDesc>
                        <!-- précisions sur la mise en page  -->
                        <layout>
                           <desc>La planche a été découpée à la cuvette et collée au sein de l’album</desc>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
               </physDesc>
               <!-- description de l'historique de l'oeuvre 
                  Dans le cas des planches d'un exemplaire, l'historique sera plutôt au niveau de la description de l'exemplaire -->
               <history>
                  <origin/>
                  <provenance/>
                  <acquisition/>
               </history>
               <listRelation>
                  <!-- attributs : @type précise le nom du modèle conceptuel ou alors indiquer ici le nom de l'entité
                                   @key et @ref pour identifier l'entité 
                                   @active correspond au sujet et @passive à l'objet (relation S-P-O) -->
                  <relation type="LRM" key="R7-is-example-of" ref="http://www.cidoc-crm.org/Property/r7-is-example-of" active="#FolRes169-pl050" passive="#plate001"/>
               </listRelation>
            </object>

