            <!-- Voir pour le nommage des @xml:id des objets -->
            <object xml:id="building001">
               <objectIdentifier>
                  <objectName>Hôtel de Thélusson</objectName>
               </objectIdentifier>
               <objectClassification>
                  <category key="E22-human-made-object" ref="http://www.cidoc-crm.org/Entity/e22-man-made-object/version-6.2.2"><desc>Human-Made Object</desc></category>
                  <classification key="nomVocabulaire" ref="lienVocabulaire">Hôtel particulier</classification>
               </objectClassification>
               <!-- Voir pour l'adresse -->
               <history>
                  <origin>
                     <placeName>
                        <address>
                           <street>30 rue de Provence</street>
                           <settlement>Paris</settlement>
                           <country>France</country>
                        </address>
                     </placeName>
                  </origin>
               </history>
               <listEvent>
                  <!-- types d'événements : commande, production, acquisition, modification, desctuction -->
                  <!-- voir pour l'événement lié à la commande @subtype : order ou finance ? -->
