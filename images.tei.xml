                     <persName ref="http://www.cidoc-crm.org/node/4435" key="P14-carried-out-by" type="patron">Marie-Jeanne Girardot de Vermenoux</persName>
                     <desc>Marie-Jeanne Girardot de Vermenoux commande l'hôtel de Thélusson à Claude-Nicolas Ledoux</desc>
                  </event>
                  <event type="CRM" key="E12-production" ref="http://www.cidoc-crm.org/Entity/e12-production/version-6.2">
                     <date>1778</date>
                     <!-- définir les rôles des participants -->
                     <!-- attribut @type : architect -->
                     <persName ref="http://www.cidoc-crm.org/node/4435" key="P14-carried-out-by" role="architect">Claude Nicolas Ledoux</persName>
                     <!-- Quelle précision dans le lieu au niveau de l'événement ? -->
                     <placeName>Paris</placeName>
                  </event>
                  <event type="CRM" key="E8-acquisition" ref="http://www.cidoc-crm.org/Entity/e8-acquisition/version-6.2"/>
                  <event type="CRM" key="E11-modification" ref="http://www.cidoc-crm.org/Entity/e11-modification/version-6.2"/>
                  <event type="CRM" key="E6-destruction" ref="http://www.cidoc-crm.org/Entity/e6-destruction/version-6.2">
                     <date>1826</date>
                     <desc>Détruit quand la rue Laffitte est prolongée jusqu’à la rue de la Victoire</desc>
                  </event>
               </listEvent>
               <listRelation>
                  <relation type="LRM" key="R3-is-realised-in" ref="http://www.cidoc-crm.org/Property/r3-is-realised-in" active="#work001" passive="#building001"/>
               </listRelation>
