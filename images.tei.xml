<TEI xmlns="http://www.tei-c.org/ns/1.0">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>Fichier d’exemple de descriptions de gravures pour le projet Ledoux</title>
         </titleStmt>
         <publicationStmt>
            <publisher>publicarchi</publisher>
            <date from="2021-03-10"/>
         </publicationStmt>
         <sourceDesc>
            <p>Document nativement numérique</p>
         </sourceDesc>
      </fileDesc>
   </teiHeader>
   <text>
      <body>
         <listObject type="albumPlates">
            <!-- type albumPlates ou Items ? ou alors est-ce qu'on type directement l'objet ? -->
            <head>Planches du recueil</head>
            <!-- première liste d'objets correspond aux planches du recueil -->

            <!-- exemple d'encodage des planches se référant à l’hôtel Thélusson (ex. INHA - Fol Res 169) -->
            <object xml:id="FolRes169-pl050">
               <!-- https://sharedocs.huma-num.fr/wl/?id=uv0tvIxjNQ7DboQQAIPLY6KffsKefQW2&path=IMG_1630.jpg -->
               <objectIdentifier>
                  <!-- Lorsque la gravure n'a pas de titre, préciser qu'il est forgé avec @type="forged" -->
                  <objectName type="forged">Vue perspective de l’hôtel Thélusson depuis la grotte</objectName>
                  <!-- quand la planche a une cote, la préciser dans idno -->
                  <idno>FolRes169-pl050</idno>
               </objectIdentifier>
               <objectClassification>
                  <!-- informations relatives à la catégorisation et à la classification de l'objet -->
                  <!-- attributs du système att.canonical : @key pour préciser le nom de l'entité ou du vocabulaire
                                                            @ref pour indiquer son uri -->
                  <!-- category correspond soit à l'entité E22 Human-Made Object soit à E20 Biological Object -->
                  <category key="E22-human-made-object" ref="http://www.cidoc-crm.org/Entity/e22-man-made-object/version-6.2.2">Human-Made Object</category>
                  <!-- classification est répétable -->
                  <classification key="nomVocabulaire" ref="lienVocabulaire"
                     >Gravure</classification>
               </objectClassification>
               <physDesc>
                  <objectDesc>
                     <!-- description matérielle -->
                     <mediumDesc>
                        <!-- description de la technique -->
                        <!-- attributs : @type pour préciser "main" ou "sub" quand il y a plusieurs techniques 
                                         @key pour le nom du vocabulaire
                                         @ref pour indiquer l'uri du vocabulaire -->
                        <medium key="nomVocabulaire" ref="lienVocabulaire">gravure ...</medium>
                     </mediumDesc>
                     <supportDesc>
                        <support>
                           <!-- description du support sur lequel l'estampe a été imprimée -->
                           <material>papier...</material>
                           <stamp>estampille des collections Jacques Doucet</stamp>
                           <watermark>...</watermark>
                        </support>
                        <extent>
                           <!-- dimensions de la planche du recueil 
                                pas forcément utile de répéter les dimensions de la page du recueil puisque cela sera indiqué dans le header -->
                           <dimensions type="albumPlate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <!-- dimensions de la feuille imprimée -->
                           <dimensions type="sheet" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <!-- dimensions de la cuvette/plaque -->
                           <dimensions type="plate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                        </extent>
                     </supportDesc>
                     <layoutDesc>
                        <!-- précisions sur la mise en page  -->
                        <layout>
                           <desc>La planche a été découpée à la cuvette et collée au sein de l’album</desc>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
               </physDesc>
               <!-- description de l'historique de l'oeuvre -->
               <history>
                  <origin/>
                  <provenance/>
                  <acquisition/>
               </history>
               <listRelation>
                  <!-- exemple de relation -->
                  <!-- attributs : @type précise le nom du modèle conceptuel ou alors indiquer ici le nom de l'entité
                                      @key et @ref pour identifier l'entité 
                                      @active correspond au sujet et @passive à l'objet (relation S-P-O) -->
                  <relation type="LRM" key="R7-is-example-of" ref="http://www.cidoc-crm.org/Property/r7-is-example-of" active="#FolRes169-pl050" passive="#plaque005"/>
                  <relation type="CRM" key="Pnn" ref="LIEN" active="SUJET" passive="OBJET"/>
               </listRelation>
            </object>

            <object xml:id="FolRes169-pl051">
               <!-- https://sharedocs.huma-num.fr/wl/?id=uv0tvIxjNQ7DboQQAIPLY6KffsKefQW2&path=IMG_1633.jpg -->
               <objectIdentifier>
                  <objectName>Plan du Rez-de-Chaussée de la maison de Madame de Thelusson, rue de Provence</objectName>
                  <idno>FolRes169-pl051</idno>
               </objectIdentifier>
               <objectClassification>
                  <category key="E22-human-made-object" ref="http://www.cidoc-crm.org/Entity/e22-man-made-object/version-6.2.2">Human-Made Object</category>
                  <classification key="nomVocabulaire" ref="lienVocabulaire">Gravure</classification>
               </objectClassification>
               <physDesc>
                  <objectDesc>
                     <mediumDesc>
                        <medium key="nomVocabulaire" ref="lienVocabulaire">gravure ...</medium>
                     </mediumDesc>
                     <supportDesc>
                        <support>
                           <material>papier...</material>
                           <stamp>estampille de la Bibliothèque d'Art et d'Archéologie</stamp>
                           <watermark>...</watermark>
                        </support>
                        <extent>
                           <dimensions type="albumPlate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <dimensions type="sheet" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <dimensions type="plate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                        </extent>
                     </supportDesc>
                     <layoutDesc>
                        <layout>
                           <desc>La planche a été découpée à la cuvette et collée au sein de l’album</desc>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
               </physDesc>
               <listRelation>
                  <relation type="LRM" key="Rnn" ref="LIEN" active="" passive=""/>
                  <relation type="CRM" key="Pnn" ref="LIEN" active="" passive=""/>
               </listRelation>
            </object>

            <object xml:id="FolRes169-pl052">
               <!-- https://sharedocs.huma-num.fr/wl/?id=uv0tvIxjNQ7DboQQAIPLY6KffsKefQW2&path=IMG_1634.jpg -->
               <objectIdentifier>
                  <objectName>Vue perspective de l’entrée de la maison de Madame de
                     Thelusson</objectName>
                  <idno>FolRes169-pl052</idno>
               </objectIdentifier>
               <objectClassification>
                  <category key="E22" ref="http://www.cidoc-crm.org/Entity/e22-man-made-object/version-6.2.2">Human-Made Object</category>
                  <classification key="nomVocabulaire" ref="lienVocabulaire">Gravure</classification>
               </objectClassification>
               <physDesc>
                  <objectDesc>
                     <mediumDesc>
                        <medium key="nomVocabulaire" ref="lienVocabulaire">gravure ...</medium>
                     </mediumDesc>
                     <supportDesc>
                        <support>
                           <material>papier...</material>
                           <stamp>estampille des collections Jacques Doucet</stamp>
                           <watermark>...</watermark>
                        </support>
                        <extent>
                           <dimensions type="albumPlate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <dimensions type="sheet" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                           <dimensions type="plate" unit="cm">
                              <height>nn</height>
                              <width>nn</width>
                           </dimensions>
                        </extent>
                     </supportDesc>
                     <layoutDesc>
                        <layout>
                           <desc>La planche a été découpée à la cuvette et collée au sein de l’album</desc>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
               </physDesc>
               <listRelation>
                  <relation type="LRM" key="Rnn" ref="LIEN" active="" passive=""/>
                  <relation type="CRM" key="Pnn" ref="LIEN" active="" passive=""/>
               </listRelation>
            </object>
         </listObject>

         <listObject type="works">
            <head>Oeuvres</head>
            <!-- Voir pour le nommage des @xml:id des objets -->
            <object xml:id="hotelThelussonW">
               <objectIdentifier>
                  <objectName>Hôtel de Thélusson</objectName>
               </objectIdentifier>
               <listEvent>
                  <!-- Les événements/activités liées à une oeuvre peuvent être listés dans listEvent -->
                  <!-- Voir pour le nommage des @xml:id des événements -->
                  <!-- attributs : @key, @ref et @type pour identifier l'entité -->
                  <event key="E65-creation" ref="http://www.cidoc-crm.org/Entity/e65-creation/version-6.2" type="Creation" xml:id="creation001">
                     <date>1778</date>
                     <persName ref="http://www.cidoc-crm.org/node/4435" key="P14-carried-out-by">Claude Nicolas Ledoux</persName>
                     <placeName/>
                  </event>
               </listEvent>
               <listRelation>
                  <relation type="CRM" key="Pnn" ref="LIEN" active="" passive=""/>
               </listRelation>
            </object>
         </listObject>

         <listObject type="buildings">
            <head>Édifices</head>
            <!-- Voir pour le nommage des @xml:id des objets -->
            <object xml:id="hotelThelussonB">
               <objectIdentifier>
                  <objectName>Hôtel de Thélusson</objectName>
               </objectIdentifier>
               <listEvent>
                  <event type="CRM" key="E12-production" ref="http://www.cidoc-crm.org/Entity/e12-production/version-6.2" xml:id="production001">
                     <date>1778</date>
                     <persName role=""/>
                     <placeName/>
                  </event>
               </listEvent>
               <listRelation>
                  <!-- exemple de relation -->
                  <relation type="LRM" key="R3-is-realised-in" ref="http://www.cidoc-crm.org/Property/r3-is-realised-in" active="#hotelThelussonW" passive="#hotelThelussonB"/>
               </listRelation>
            </object>
         </listObject>

         <listObject type="plates">
            <head>Manifestations gravées (plaques)</head>
            <!-- Voir pour le nommage des @xml:id des objets -->
            <object xml:id="plaque005">
               <objectIdentifier>
                  <objectName>Plaque de la Vue perspective de l’hôtel Thélusson depuis la
                     grotte</objectName>
               </objectIdentifier>
               <listEvent>
                  <event type="CRM" key="E12-production" ref="http://www.cidoc-crm.org/Entity/e12-production/version-6.2" xml:id="production001">
                     <date>1778</date>
                     <persName role=""/>
                     <placeName/>
                  </event>
               </listEvent>
               <listRelation>
                  <!-- exemple de relation -->
                  <relation type="LRM" key="R3-is-realised-in" ref="http://www.cidoc-crm.org/Property/r3-is-realised-in" active="#hotelThelussonW" passive="#plaque005"/>
               </listRelation>
            </object>
         </listObject>

         <listPerson>
            <!-- liste qui sera peut être dans le header ? Permettra ensuite de faire des relations entre les oeuvres, Ledoux et le graveur -->
            <!-- voir le nommage des @xml:id -->
            <person xml:id="Ledoux">
               <name>Claude Nicolas Ledoux</name>
            </person>
            <person xml:id="Hulk">
               <name>Abraham Jacobsz Hulk</name>
            </person>
         </listPerson>
      </body>
   </text>
</TEI>
